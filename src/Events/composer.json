{
    "name": "spiral/events",
    "type": "library",
    "description": "Spiral Framework: Event Dispatcher",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/events"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spiral/attributes": "^2.8 || ^3.0",
        "spiral/tokenizer": "^3.0",
        "psr/event-dispatcher": "^1.0",
        "psr/event-dispatcher-implementation": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Events\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Events\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.14.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
